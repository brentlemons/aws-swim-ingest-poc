   <import namespace="http://www.fixm.aero/foundation/3.0" schemaLocation="./../../core/foundation/Foundation.xsd"/>
   <annotation>
      <documentation>
         Defines NAS specific time concepts that are shared among several elements. 
      </documentation>
   </annotation>

   <complexType name="NasRunwayActualType">
      <annotation>
         <documentation>
            Contains information about actual runway time. 
            .Runway Arrival Time - Traffic Flow Management System Actual: The actual runway arrival 
            time, as determined by Traffic Flow Management System (TFMS). 
            .Runway Arrival Time - Airspace User Actual: The actual time of runway arrival, as 
            provided by the Airspace User. 
         </documentation>
      </annotation>
      <simpleContent>
         <extension base="ff:TimeType">
            <attribute name="source" type="nas:SourceType" use="optional" > 
               <annotation>
                  <documentation>
                     Describes the source of the actual runway time information 
                  </documentation>
               </annotation>
            </attribute>
         </extension>
      </simpleContent>
   </complexType>

   <simpleType name="SourceType">
      <annotation>
         <documentation>
            Source of the actual runway time information 
         </documentation>
      </annotation>
      <restriction base="xs:string">
         <enumeration value="TFMS"/>
         <enumeration value="AIRSPACE_USER"/>
      </restriction>
   </simpleType>
</schema>
